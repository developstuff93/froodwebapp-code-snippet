{
  "name": "frood-web-app",
  "version": "0.0.9",
  "description": "Frood Web Application. Build With ReactJS",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node index.js",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "prod": "npm run build:webpack && cross-env NODE_ENV=production node prodServer.js",
    "lint": "eslint src",
    "start": "node prodServer.js",
    "test": "cross-env NODE_ENV=test jest",
    "flow": "flow",
    "flow-stop": "flow stop"
  },
  "engines": {
    "node": "6.10.3",
    "npm": "5.0.3"
  },
  "private": true,
  "author": "Jose Arias",
  "browserslist": {
    "production": [
      "last 2 version",
      "ie 9"
    ],
    "development": [
      "last 1 version"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/test-setup.js"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.js"
    }
  },
  "dependencies": {
    "antd": "2.12.7",
    "autoprefixer": "7.1.2",
    "axios": "0.16.2",
    "classnames": "2.2.5",
    "element-resize-event": "2.0.9",
    "express": "4.15.4",
    "flexboxgrid": "6.3.1",
    "history": "4.6.3",
    "immutability-helper": "2.3.1",
    "js-cookie": "2.1.4",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "object-diff": "0.0.4",
    "prop-types": "15.5.10",
    "proxy-middleware": "0.15.0",
    "react": "15.6.1",
    "react-autocomplete": "1.7.1",
    "react-dnd": "2.4.0",
    "react-dnd-html5-backend": "2.4.1",
    "react-dom": "15.6.1",
    "react-flexbox-grid": "1.1.3",
    "react-fontawesome": "1.6.1",
    "react-redux": "5.0.6",
    "react-router-dom": "4.1.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-tap-event-plugin": "2.0.1",
    "react-telephone-input": "4.1.2",
    "redux": "3.7.2",
    "redux-form": "7.0.3",
    "redux-saga": "0.15.6",
    "screenfull": "3.3.1"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-imports": "1.4.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "compression-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.5",
    "eslint": "4.5.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-css-modules": "2.7.4",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.2.1",
    "eslint-plugin-redux-saga": "0.4.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "flow-bin": "0.53.1",
    "jest": "^20.0.4",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.6",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.5",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.7.1",
    "webpack-merge": "4.1.0"
  }
}
